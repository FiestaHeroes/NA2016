open [main]
    var Wait            ""
        InterruptBlock  ""
        InterruptArg    ""
        KQ_GB_Spider  "".
     ;  HiddenEvent01   1.
     ;  HiddenEvent02   aaaa. 

    waitlogin Wait.
    if Wait == 0                            ; 일정시간 지나도록 로긴 안함
    then open
        call "QuestFail".
    close


    pause Sec 60.

    scriptfile "KQUnderHall2".

    timelimit Min 30.

    call "First".
    call "Second".
    call "Third".
    call "Forth".
    call "Fifth".
    call "Sixth".
    call "Seventh".
    call "Eighth".
    call "Nineth".
    call "Tenth".
    call "Eleventh".
    call "Twelveth".
    call "Thirteenth".
    call "Forteenth".
    call "Fifteenth".
    call "Sixteenth".
    call "Seventeenth".
    call "Eighteenth".
    call "Nineteenth".
    call "Twelve".
    call "TwelveOne".
    call "TwelveTwo".

close
